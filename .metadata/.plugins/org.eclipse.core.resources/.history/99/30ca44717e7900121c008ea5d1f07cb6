package com.subject17.jdfs.client.net;

public class LanguageProtocol {
	final static String SYN="jdfs-Attempt-Connect";
	final static String ACK="jdfs-Acknowledge-Connect";
	final static String UNKNOWN="jdfs-Unknown-Communication";
	
	public String handleResponse(String s) {
		switch (s) {
			case SYN: return ACK;
		}
		return UNKNOWN;
	}
}
