package com.subject17.jdfs.client.settings;

import org.w3c.dom.Document;

public class PeerSettingsReader extends SettingsReader {
	private String peerSettingsFile;
	private String peerSettingsPath;
	
	private Document peerSettingsDoc;
	
	public PeerSettingsReader(String filename, String pathName){
		peerSettingsFile = filename;
		peerSettingsPath = pathName;
		readDocument();
	}
}
