package com.subject17.jdfs.client.settings;

public class UserSettingsReader extends SettingsReader {
	public String filename;
	public String path;
	
	Document usersDoc;
	
	public UserSettingsReader(String pathloc, String fname){
		path = pathloc;
		filename = fname;
	}
	
	public Document getUsersDocument() {return GetDoc();}
}
