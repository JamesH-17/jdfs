package com.subject17.jdfs.client.settings;

import java.io.File;

import org.w3c.dom.Document;

public class UserSettingsReader extends SettingsReader {
	private String filename;
	private String path;
	private File usersFile;
	
	Document usersDoc;
	
	public UserSettingsReader(String pathloc, String fname){
		path = pathloc;
		filename = fname;
		usersFile = new File(pathloc, fname);
		usersDoc = getUsersDocument();
	}
	
	public Document getUsersDocument() {return GetDocument(usersFile);}
}
