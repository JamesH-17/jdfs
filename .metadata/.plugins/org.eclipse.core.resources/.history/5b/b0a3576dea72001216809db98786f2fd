package com.subject17.jdfs.client.install;

import java.io.File;

public class Settings {
	private String settingsFile;
	
	public Settings() {
		settingsFile="";
		
	}
	
	public String getSettingsFile() {
		return settingsFile;
	}
	
	private boolean setSettingsFile(String newSettingsFile) {
		if ((new File(newSettingsFile).exists())) {
			settingsFile = newSettingsFile;
		}
		else return false;
	}
}
